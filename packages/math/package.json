{
  "name": "@dalpeng/math",
  "type": "module",
  "version": "0.0.1",
  "description": "A very simple typescript math library for dalpeng engine, including Vector, Matrix and Quaternion",
  "main": "./dist/math.umd.cjs",
  "module": "./dist/math.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/math.js",
      "require": "./dist/math.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/threedalpeng/CGD-WebGL-Math.git"
  },
  "bugs": {
    "url": "https://github.com/threedalpeng/CGD-WebGL-Math/issues"
  },
  "homepage": "https://github.com/threedalpeng/CGD-WebGL-Math#readme",
  "keywords": [
    "dalpeng"
  ],
  "author": "Kim Jeong-Won <threedalpeng@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui --api.host 172.105.229.189 --api.port 5050"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.4",
    "@vitest/ui": "^0.20.3",
    "rollup": "^2.77.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.20.3"
  },
  "dependencies": {
    "@types/node": "^18.6.3"
  }
}
