{
  "name": "@dalpeng/math",
  "type": "module",
  "version": "0.1.0",
  "description": "A very simple typescript math library for dalpeng engine, including Vector, Matrix and Quaternion",
  "main": "./dist/math.umd.cjs",
  "module": "./dist/math.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/math.js",
      "require": "./dist/math.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/threedalpeng/dalpeng.git"
  },
  "bugs": {
    "url": "https://github.com/threedalpeng/dalpeng/issues"
  },
  "homepage": "https://github.com/threedalpeng/dalpeng/packages/math#readme",
  "keywords": [
    "dalpeng",
    "math"
  ],
  "author": "Kim Jeong-Won <threedalpeng@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui --api.host 172.105.229.189 --api.port 5050"
  },
  "devDependencies": {
    "@vitest/ui": "^0.28.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.3",
    "vite-plugin-dts": "^1.7.2"
  },
  "dependencies": {}
}
